<templateSet group="user">
  <template name="add" value="/**&#10;* $user$&#10;* $date$&#10;*/" description="" toReformat="false" toShortenFQNames="true">
    <variable name="user" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="sysout" value="System.out.print(&quot;$user$&quot;);" toReformat="false" toShortenFQNames="true">
    <variable name="user" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>